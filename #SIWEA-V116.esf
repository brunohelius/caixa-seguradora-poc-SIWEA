:func      name      = SIM0ZZZS03A       
           date      = '06/17/2003' time = '19:03:39' option = EXECUTE  
           desc      = 'gera acomp sinistro (NOVO)'
           refine    = N  
:before.
;
; /* gera acompanhamento do sinistro (novo)
;
SET LBWCT001 EMPTY;
SET LBWACOMO EMPTY;
SET LBWCT002 EMPTY;
;
MOVE 'SI' TO LBWCT001.IN-SISTEMA;
MOVE EZEUSRID TO LBWCT001.IN-USUARIO;
MOVE 1 TO LBWCT001.IN-OPERACAO;  /* inclusao
;
MOVE SIM0W099.COD_FONTE TO LBWACOMO.COD_FONTE;
MOVE SIM0W099.NUM_PROTOCOLO_SINI TO LBWACOMO.NUM_PROTOCOLO_SINI;
MOVE SIM0W099.DAC_PROTOCOLO_SINI TO LBWACOMO.DAC_PROTOCOLO_SINI;
MOVE SIM0W099.WCOD_EVENTO TO LBWACOMO.COD_EVENTO; /* pre-liberacao parcial
MOVE SIM0W099.DATA_MOV_ABERTO TO LBWACOMO.DATA_MOVTO_SINIACO;
MOVE ' ' TO LBWACOMO.DESCR_COMPLEMENTAR;
MOVE EZEUSRID TO LBWACOMO.COD_USUARIO;
;
CALL PTACOMOS LBWCT001,LBWACOMO,LBWCT002 (REPLY;
;
IF EZERT8 NE '00000000';
  EZEREPLY = 1;
  EZEROLLB();
  MOVE 'PROBLEMAS NA SUBROTINA: PTACOMOS' TO SIM0W099.MENSAGEM;
  EZERTN();
END;
;
IF LBWCT002.OUT-COD-RETORNO NE 0;
  EZEREPLY = 1;
  EZEROLLB();
  MOVE LBWCT002.OUT-MENSAGEM TO SIM0W099.MENSAGEM;
  EZERTN();
