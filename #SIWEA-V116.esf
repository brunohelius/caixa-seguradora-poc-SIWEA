:func      name      = SIWES09A          
           date      = '05/08/2003' time = '12:04:45' option = EXECUTE  
           refine    = N  
:before.
SET V1HISTSINI EMPTY;
;
MOVE 0 SIWEW001.W01P1505;        /* GUARDA SOMATORIO
MOVE SIWEM020.NUM_APOL_SINISTRO V1HISTSINI.NUM_APOL_SINISTRO;
;
SIWEP0A();                       /* DECLARA V1HISTSINI
SIWEP0B();                       /* ACESSA V1HISTSINI
;
WHILE V1HISTSINI NOT NRF;
;
;  /* CONVERTE VAL_OPERACAO PARA QTDE. DE MOEDA EM W02P1505
;
  MOVE 0 SIWEW001.W02P1505;      /* GUARDA TOTPAGBT EM QTDE DE MOEDA
;
  MOVE V1MESTSINI.COD_MOEDA_SINI V1MOEDA.CODUNIMO;
  MOVE V1HISTSINI.DTMOVTO V1MOEDA.DTINIVIG;
  SIWEP14();                     /* ACESSA V1MOEDA
;
  SIWEW001.W02P1505 = V1HISTSINI.VAL_OPERACAO / V1MOEDA.VLCRUZAD;
;
;  /* ACUMULA VAL_OPERACAO EM W01P1505
;
  SIWEW001.W01P1505 = SIWEW001.W01P1505 + SIWEW001.W02P1505;
;
  SIWEP0B();                     /* ACESSA V1HISTSINI
;
END;
;
; /* ADICIONA O CAMPO TOTPAGBT E O VALOR INFORMADO
;
MOVE 0 SIWEW001.W01P1502;        /* GUARDA SOMATORIA EM MOEDA CORRENTE
MOVE 0 SIWEW001.W02P1502;        /* GUARDA VALSEGBT EM MOEDA CORRENTE
MOVE 0 SIWEW001.W02P1505;        /* GUARDA VAL_OPERACAO EM QTDE DE MOEDA
;
MOVE V1MESTSINI.COD_MOEDA_SINI V1MOEDA.CODUNIMO;
MOVE V1SISTEMA.DTMOVABE V1MOEDA.DTINIVIG;
SIWEP14();                       /* ACESSA V1MOEDA
;
SIWEW001.W02P1505 = SIWEM020.VAL_OPERACAO / V1MOEDA.VLCRUZAD;
;
SIWEW001.W01P1505 = SIWEW001.W01P1505 + V1MESTSINI.TOTPAGBT
  + SIWEW001.W02P1505;
;
; /* OBTEM SOMATORIA EM MOEDA CORRENTE
;
SIWEW001.W01P1502 = SIWEW001.W01P1505 * V1MOEDA.VLCRUZAD;
;
; /* TRANSFORMA VALSEGBT EM MOEDA CORRENTE
;
MOVE V1MESTSINI.NUM_APOLICE TO V0ENDOSSO.NUM_APOLICE;
MOVE V1MESTSINI.DATORR TO V0ENDOSSO.DTINIVIG;
;
SIWEPB030();       /* Acessa V0ENDOSSO
;
MOVE V0ENDOSSO.COD_MOEDA_IMP V1MOEDA.CODUNIMO;
MOVE V1SISTEMA.DTMOVABE V1MOEDA.DTINIVIG;
SIWEP14();                       /* ACESSA V1MOEDA
;
SIWEW001.W02P1502 = V1MESTSINI.VALSEGBT * V1MOEDA.VLCRUZAD;
;
IF SIWEW001.W01P1502 GT SIWEW001.W02P1502;
  IF SIWEW001.W01N0001 EQ 0;
    MOVE 'TECLE ENTER PARA CONFIRMAR VALOR INFORMADO MAIOR QUE I.S.'
      TO SIWEM020.EZEMSG;
    SET SIWEM020.VAL_OPERACAO CURSOR,BRIGHT;
    MOVE 1 TO SIWEW001.W01N0001;
    EZEFLO;
  END;
END;
;
:ebefore.
:efunc.

