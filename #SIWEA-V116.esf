:func      name      = SIM0ZZZS03B       
           date      = '06/17/2003' time = '19:03:39' option = EXECUTE  
           desc      = 'atualiza fase do sinistro'
           refine    = N  
:before.
;
; /* verifica atualizacao da fase do processo
;
SET SI_REL_FASE_EVENTO EMPTY;
;
MOVE LBWACOMO.COD_EVENTO TO SI_REL_FASE_EVENTO.COD_EVENTO;
MOVE SIM0W099.DATA_MOV_ABERTO TO SI_REL_FASE_EVENTO.DATA_INIVIG_REFAEV;
SIM0ZZZS03P01();                 /* declara SI_REL_FASE_EVENTO
;
IF SIM0W099.MENSAGEM NE ' ';
  EZERTN();
END;
;
SIM0ZZZS03P02();                 /* le SI_REL_FASE_EVENTO
;
IF SIM0W099.MENSAGEM NE ' ';
  EZERTN();
END;
;
SET LBWCT001 EMPTY;
SET LBWFASES EMPTY;
SET LBWCT002 EMPTY;
;
MOVE 'SI' TO LBWCT001.IN-SISTEMA;
MOVE EZEUSRID TO LBWCT001.IN-USUARIO;
;
MOVE SIM0W099.COD_FONTE TO LBWFASES.COD_FONTE;
MOVE SIM0W099.NUM_PROTOCOLO_SINI TO LBWFASES.NUM_PROTOCOLO_SINI;
MOVE SIM0W099.DAC_PROTOCOLO_SINI TO LBWFASES.DAC_PROTOCOLO_SINI;
MOVE LBWACOMO.COD_EVENTO TO LBWFASES.COD_EVENTO;
MOVE LBWACOMO.NUM_OCORR_SINIACO TO LBWFASES.NUM_OCORR_SINIACO;
;
WHILE SI_REL_FASE_EVENTO NOT NRF;
;
;  /* se for abertura da fase, inclui com data de fechamento nula
;  /* se for fechamento da fase, altera data de fechamento
;
  MOVE SI_REL_FASE_EVENTO.COD_FASE TO LBWFASES.COD_FASE;
  MOVE SI_REL_FASE_EVENTO.DATA_INIVIG_REFAEV
    TO LBWFASES.DATA_INIVIG_REFAEV;
  MOVE SIM0W099.DATA_MOV_ABERTO TO LBWFASES.DATA_ABERTURA_SIFA;
;
  IF SI_REL_FASE_EVENTO.IND_ALTERACAO_FASE EQ '1'; /* abertura
    MOVE '9999-12-31' TO LBWFASES.DATA_FECHA_SIFA;
    MOVE 1 TO LBWCT001.IN-OPERACAO; /* inclusao
  ELSE;
    IF SI_REL_FASE_EVENTO.IND_ALTERACAO_FASE EQ '2'; /* fechamento
      MOVE SIM0W099.DATA_MOV_ABERTO TO LBWFASES.DATA_FECHA_SIFA;
      MOVE 2 TO LBWCT001.IN-OPERACAO; /* alteracao
    END;
  END;
;
  /* verifica se fase jê esta aberta ou encerrada
;
  SET SI_SINISTRO_FASE EMPTY;
;
  MOVE SIM0W099.COD_FONTE TO SI_SINISTRO_FASE.COD_FONTE;
  MOVE SIM0W099.NUM_PROTOCOLO_SINI TO SI_SINISTRO_FASE.NUM_PROTOCOLO_SINI
    ;
  MOVE SIM0W099.DAC_PROTOCOLO_SINI TO SI_SINISTRO_FASE.DAC_PROTOCOLO_SINI
    ;
  MOVE SI_REL_FASE_EVENTO.COD_FASE TO SI_SINISTRO_FASE.COD_FASE;
  MOVE LBWACOMO.COD_EVENTO TO SI_SINISTRO_FASE.COD_EVENTO;
  SIM0ZZZS03P03();               /* le SI_SINISTRO_FASE
;
  IF SIM0W099.MENSAGEM NE ' ';
    EZERTN();
  END;
;
  IF SI_SINISTRO_FASE IS NRF;
;
    CALL PTFASESS LBWCT001,LBWFASES,LBWCT002 (NONCSP,NOMAPS,REPLY;
;
    IF EZERT8 NE '00000000';
      EZEREPLY = 1;
      EZEROLLB();
      MOVE 'PROBLEMAS NA SUBROTINA: PTFASESS' TO SIM0W099.MENSAGEM;
      EZERTN();
    END;
;
    IF LBWCT002.OUT-COD-RETORNO NE 0;
      EZEREPLY = 1;
      EZEROLLB();
      MOVE LBWCT002.OUT-MENSAGEM TO SIM0W099.MENSAGEM;
      EZERTN();
    END;
  END;
;
  SIM0ZZZS03P02();               /* le SI_REL_FASE_EVENTO
;
  IF SIM0W099.MENSAGEM NE ' ';
    EZERTN();
  END;
END;
